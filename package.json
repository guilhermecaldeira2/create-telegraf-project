{
  "name": "@guitdevbr/chatbot-cli",
  "version": "1.0.0",
  "description": "A CLI to bootstrap telegraf projects",
  "main": "bin/index.js",
  "bin": {
    "create-telegraf-project": "./bin/index.js",
    "@guitdevbr/create-telegraf-project": "./bin/index.js"
  },
  "scripts": {
    "build": "babel src --extensions \" .js,.ts\" --out-dir bin --copy-files --no-copy-ignored && cp -r templates bin/templates",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/index.ts"
  },
  "keywords": [
    "CLI",
    "telegraf",
    "chatbot",
    "create-telegraf-project"
  ],
  "author": "guitdevbr",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/ncp": "^2.0.4",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.1.0",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "ncp": "^2.0.0"
  }
}
